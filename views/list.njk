{% extends "layout.njk" %}

{% block content %}
<main class="wrapper">
    <h1>{{ list.listname }}</h1>

    <h2>Points: {{ list.pointsValue }}pts</h2>

    <h3>List by: <a href="../users/{{ list.user_id }}">{{ list.user_id }}</a></h3>

    <h3>Composition:</h3>
    <p>{{ list.composition }}</p>
</main>
{% endblock %}